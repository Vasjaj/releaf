.ui-widget-overlay
{
  background-color: rgba(0, 0, 0, 0.3);
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  position: absolute;
  z-index: 90;
}
.ui-dialog
{
  background-color: white;
  box-shadow: 1px 3px 5px 0px rgba(0, 0, 0, 0.6);
  max-width: 700px;
  z-index: 100;
}
.ui-dialog > .ui-dialog-titlebar
{
  background: black;
  height: 38px;
  position: relative;
}
.ui-dialog > .ui-dialog-titlebar .ui-dialog-title
{
  font-size: 1.215em;
  color: white;
  line-height: 38px;
  padding-left: 15px;
  margin-right: 55px;
  white-space: nowrap;
  overflow: hidden;
  display: block;
}
.ui-dialog > .ui-dialog-titlebar .ui-dialog-titlebar-close
{
  float: right;
  display: block;
  width: 39px;
  height: 38px;
  line-height: 38px;
  color: #34958d;
  text-align: center;
  text-decoration: none;
  border-left: 1px solid #515151;
  display: block;
  position: absolute;
  font-size: 1.142em;
  right: 0px;
  top: 0px;
}
.ui-dialog > .ui-dialog-titlebar .ui-dialog-titlebar-close:hover
{
  background-color: #3a3a3a;
  color: #37b1a7;
  text-decoration: none;
}
.ui-dialog > .ui-dialog-content button,
.ui-dialog > .ui-dialog-content .button
{
  color: white;
  text-decoration: none;
  font-size: 1em;
  display: block;
  line-height: 1.429em;
  padding: 10px 20px 10px 15px;
  background-color: #37b1a7;
}
.ui-dialog > .ui-dialog-content button:hover,
.ui-dialog > .ui-dialog-content .button:hover
{
  text-decoration: none;
  background-color: #269990;
}
