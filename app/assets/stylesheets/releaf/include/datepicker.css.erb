#ui-datepicker-div
{
  background-color: white;
  box-shadow: 1px 3px 5px 0px rgba(0, 0, 0, 0.6);
}
.ui-datepicker-header
{
  background: black;
  height: 38px;
}
.ui-datepicker-header .ui-datepicker-next,
.ui-datepicker-header .ui-datepicker-prev
{
  float: left;
  display: block;
  width: 39px;
  height: 38px;
  line-height: 38px;
  color: white;
  text-align: center;
  text-decoration: none;
}
.ui-datepicker-header .ui-datepicker-next:hover,
.ui-datepicker-header .ui-datepicker-prev:hover
{
  background-color: #3a3a3a;
  color: #37b1a7;
  text-decoration: none;
}
.ui-datepicker-header .ui-datepicker-prev
{
  float: left;
}
.ui-datepicker-header .ui-datepicker-next
{
  float: right;
}
.ui-datepicker-header .ui-datepicker-next span,
.ui-datepicker-header .ui-datepicker-prev span
{
  display: none;
}
.ui-datepicker-header .ui-datepicker-title
{
  text-align: center;
  font-size: 1.215em;
  color: white;
}
.ui-datepicker-header .ui-datepicker-title select
{
  margin-top: 2px;
  font-size: 0.824em;
}
.ui-datepicker-calendar
{
  border-collapse: collapse;
}
.ui-datepicker-calendar thead
{
  color: #6c7174;
  background-color: #e7ecf0;
  border: 1px solid #e7ecf0;
}
.ui-datepicker-calendar thead th
{
  font-weight: normal;
  font-size: 0.64286em;
  text-transform: uppercase;
  line-height: 1em;
  padding: 7px 11px 6px;
  text-shadow: 0px 1px 2px rgba(255, 255, 255, 0.55);
}
.ui-datepicker-calendar td
{
  border: 1px solid #dedede;
  text-align: center;
  vertical-align: middle;
  padding: 6px 11px;
}
.ui-datepicker-calendar td a
{
  color: black;
  text-decoration: none;
  font-size: 0.93em;
  font-weight: bold;
  line-height: 1em;
}
.ui-datepicker-calendar td.ui-datepicker-current-day
{
  background-color: #dedede;
  text-shadow: 0px 1px 2px rgba(255, 255, 255, 0.55);
}
.ui-datepicker-calendar td.ui-datepicker-current-day:hover,
.ui-datepicker-calendar td:hover
{
  background-color: #37b1a7;
  text-shadow: 0px -1px 1px rgba(0, 0, 0, 0.55);
}
.ui-datepicker-calendar td:hover a
{
  color: white;
}
