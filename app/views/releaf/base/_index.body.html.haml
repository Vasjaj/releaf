%h2.header
  = t("all_title", :scope => 'admin.global')
  %span.totals
    = count \
    = t("resources_found", :scope => 'admin.global')

- if resources.empty?
  .releaf_table.no_border
    .nothing_found= t("nothing_found", :scope => "admin.global")
- else
  = render "index.table", :resources => resources, :count => count
