@font-face {
    font-family: 'robotoregular';
    src: url('<%= asset_path("releaf/roboto-regular-webfont.eot") %>');
    src: url('<%= asset_path("releaf/roboto-regular-webfont.eot?#iefix") %>') format('embedded-opentype'),
         url('<%= asset_path("releaf/roboto-regular-webfont.woff") %>') format('woff'),
         url('<%= asset_path("releaf/roboto-regular-webfont.ttf") %>') format('truetype'),
         url('<%= asset_path("releaf/roboto-regular-webfont.svg#robotoregular") %>') format('svg');
    font-weight: normal;
    font-style: normal;
}
